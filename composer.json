{
  "name": "apie/country-and-phone-number",
  "description": "Composer package of the apie library: country and phonenumber",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\CountryAndPhoneNumber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\CountryAndPhoneNumber\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "giggsey/libphonenumber-for-php": "^8.0",
    "prinsfrank/standards": "^1.0.0"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "cebe/php-openapi": "^1.7",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}