{
  "name": "apie/country-and-phone-number",
  "description": "Composer package of the apie library: country and phonenumber",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\CountryAndPhoneNumber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\CountryAndPhoneNumber\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\CountryAndPhoneNumber\\BelgianPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\BritishPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\CanadianPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\ChinesePhoneNumber",
      "Apie\\CountryAndPhoneNumber\\CountryAndPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\DutchPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\FrenchPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\GermanPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\InternationalPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\JapanesePhoneNumber",
      "Apie\\CountryAndPhoneNumber\\MexicanPhoneNumber",
      "Apie\\CountryAndPhoneNumber\\USPhoneNumber"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "giggsey/libphonenumber-for-php": "^8.13.37",
    "prinsfrank/standards": "^3.3.0"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "devizzent/cebe-php-openapi": "^1.1.2",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}